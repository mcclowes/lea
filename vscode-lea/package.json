{
  "name": "lea-language",
  "displayName": "Lea Language",
  "description": "Syntax highlighting for the Lea programming language - a pipe-oriented functional language",
  "version": "0.1.0",
  "publisher": "mcclowes",
  "author": {
    "name": "Max Clayton Clowes"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcclowes/lea"
  },
  "bugs": {
    "url": "https://github.com/mcclowes/lea/issues"
  },
  "homepage": "https://github.com/mcclowes/lea#readme",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "lea",
    "functional",
    "pipe",
    "syntax highlighting"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "contributes": {
    "languages": [
      {
        "id": "lea",
        "aliases": ["Lea", "lea"],
        "extensions": [".lea"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "lea",
        "scopeName": "source.lea",
        "path": "./syntaxes/lea.tmLanguage.json"
      }
    ]
  }
}
